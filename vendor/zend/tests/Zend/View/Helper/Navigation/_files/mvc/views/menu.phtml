<?php
$isContainer = $this->vars('container') instanceof \Zend\Navigation\Container
             ? 'yes'
             : 'no';

$pages = array();
foreach ($this->vars('container') as $page) {
    $pages[] = $page->getLabel();
}
$pages = implode(', ', $pages);
?>
Is a container: <?php echo $isContainer . "\n"; ?>
Pages: <?php echo $pages; ?>
