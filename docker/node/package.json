{
    "name": "organist-term",
  "version": "0.0.2",
  "description": "Read only terminal in browser. Command provision from MySQL, logs stored in same table",
  "main": "server.js",
  "dependencies": {
    "term.js": "0.0.7",
    "socket.io": "1.3.7",
    "express": "4.13.3",
    "pty.js": "0.3.0",
    "mysql": "2.9.0",
    "http": "0.0.0",
    "fs": "0.0.2"
  },
  "devDependencies": {},
  "scripts": {
    "start": "node server.js --dump --debug"
  },
  "repository": {
    "type": "git",
    "url": "nnb"
  },
  "keywords": [
    "console",
    "browser",
    "readonly",
    "simultaneously",
    "parallel",
    "access",
    "ctrl-c",
    "command execution",
    "command logging"
  ],
